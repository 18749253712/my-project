/* pages/shouye/shouye.wxss */
.box{
  width: 100%;
  height: 100%;
}
.box .top{
  width: 100%;
  height: 360px;
}
.box .top{
  width: 100%;
  height: calc(100vw*360/750);
}
.box .top swiper-item image{
  width: 100%;
   height: calc(100vw*360/750);
}
.begin{
  width: 100%;
  /* height: 200rpx; */
  /* border: 1px solid black; */
  /* margin: 0 auto; */
  padding-left: 180rpx;
}
.begin image{
  width: 400rpx;
  height: 320rpx;
  margin-top: -100rpx;
}
.h1{
  width: 85%;
  height: 120rpx;
  /* border: 1px solid pink; */
  margin: 0 auto;
  margin-top: -80rpx;
  text-align: center;
}
.h1 text{
  margin-left: 8px;
}
.h1 view:last-child{
  color: gray;
  font-size: 14px;
  margin-top: 10px;
}
.section{
  width: 100%;
  height: 240rpx;
  /* border: 1px solid pink; */
  display: flex;
}
.section .beginview{
  width: 33.33%;
  height: 240rpx;
}
.section .beginview image{
  width: 100%;
  height: 100%;
}
.section .beginview view{
  margin-left: 20rpx;
}
.section .beginview .one{
  margin-top: -100rpx;
}
.footer{
  width: 90%;
  height: 320rpx;
  margin: 0 auto;
  display: flex;
}
.footer .left{
  width: 50%;
  height: 320rpx;
}
.footer .left image{
  width: 100%;
  height: 100%;
}
.footer .right{
  width: 50%;
  height: 320rpx;
}
.footer .right .up{
  width: 100%;
  height: 100rpx;
  /* border: 1px solid black; */
}
.footer .right .down{
  width: 100%;
  height: 150rpx;
  color: gray;
  font-size: 14px;
  /* margin-top: 20rpx; */
  /* border: 1px solid black; */
}
.footer .right .click{
  width: 60%;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  margin: 0 auto;
  border: 1rpx solid black;
}
.show{
  width: 90%;
  height: 380rpx;
  border: 3px solid whitesmoke;
  margin: 0 auto;
  margin-bottom: 20rpx;
}
.show image{
  width: 100%;
  height: 85%;
  border-radius: 10rpx 10rpx 0rpx 0rpx;
}
.show view{
  margin-left: 40rpx;
}
.mv{
  width: 80%;
  height: 350rpx;
  /* border: 1px solid pink; */
  margin: 0 auto;
  margin-top: 20px;
  /* background-color: pink; */
}
.mv image{
  width: 100%;
  height: 100%;
}
.mv video{
  max-width: 100%;
  max-height: 340rpx;
}
.form{
  width: 100%;
  height: 60rpx;
  border-bottom:3px solid whitesmoke ;
  /* background-color: pink; */
}
.form1{
  width: 100%;
  height: 80rpx;
  background-color: rgb(247, 199, 208);
  margin-top: 10rpx;
  text-align: center;
}
.content1{
  width: 90%;
  height: 1050rpx;
  border: 3px solid whitesmoke;
  margin: 0 auto;
}
.content1 view{
  width: 90%;
  height: 80rpx;
  /* border: 1px solid black; */
  margin: 0 auto;
  line-height: 80rpx;
}
.content1 view text{
  color: red;
}
.content1 view input{
  width: 100%;
  height: 80rpx;
  background-color: rgb(245, 243, 243);
  padding-left: 20rpx;
  /* border: 1px solid blue; */
}
.content1 .last input{
  width: 100%;
  height: 180rpx;
  /* line-height: 60rpx; */
  background-color: rgb(245, 243, 243);
  padding-left: 20rpx;
  /* margin-top: -40rpx; */
}
.content1 view:last-child{
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  background-color: black;
  color: white;
  text-align: center;
  margin: 0 auto;
  margin-top: 150rpx;
  border-radius: 40rpx;
}
.ss{
  border: 1px solid blue;
}
.big{
  width: 100%;
  height: 100%;
  background-color: black;
  position: fixed;
  top: 0;
  left: 0;
  opacity: 0.9;
  z-index: 999;
  /* display: none; */
}
.big1{
  width: 100%;
  height: 100%;
  background-color: gainsboro;
  position: fixed;
  top: 0;
  left: 0;
  opacity: 0.8;
  z-index: 2;
  display: none;
}
.hide{
  width: 100%;
  height: 180rpx;
  background-color: white;
  z-index: 999;
  position: fixed;
  bottom: 0;
  opacity: 1;
  /* display: none; */
}
.hide1{
  width: 100%;
  height: 180rpx;
  background-color: white;
  z-index: 999;
  position: absolute;
  bottom: 0rpx;
  opacity: 1;
  display: none;
}
.hide view{
  text-align: center;
  line-height: 80rpx;
}